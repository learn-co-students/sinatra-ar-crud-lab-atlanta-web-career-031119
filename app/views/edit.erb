<form action="/articles/<%=@article.id%>" method="post">
<input id="hidden" type="hidden" name="_method" value="patch">
<p> :Title <input type="text" name="title" id=article[title]> </p>


<p> Content: <input type="text" name="content" id=article[content]> </p>

<input type="submit" value="submit">

</form>

<p> :id <%=@article.id%>
    title: <%=@article.title%>
    content: <%=@article.content%>
</p>
